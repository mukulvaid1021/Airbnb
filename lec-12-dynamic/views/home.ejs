<%- include('partials/head') %>
</head>
<body class="bg-gray-50 min-h-screen font-sans">
<%- include('partials/nav') %>
  <!-- Top header bar -->


  <!-- Main content with padding to avoid overlap with fixed nav -->
  <main class="pt-20 px-6 flex flex-col items-center">

    <header class="mb-8">
      <h2 class="text-red-700 text-5xl font-bold drop-shadow-md">Here are our Registered homes:</h2>
    </header>

    <ul class="w-full max-w-xl space-y-4">
      <% registeredhomes.forEach(home =>{ %>
        <li class="bg-white rounded-lg shadow-md p-5 text-gray-800 text-xl font-medium hover:shadow-xl transition-shadow duration-300 cursor-default select-none">
          <%= home.housename %>
        </li>
      <% }) %>
    </ul>
    
  </main>

</body>
</html>

